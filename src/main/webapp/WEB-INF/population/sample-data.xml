	</bean>
	
		<bean id="shout4" class="acme.entities.shouts.Shout">
		<property name="moment" value="2020/02/08 12:48"/>
		<property name="author" value="Diego Plaza"/>
		<property name="text" value="I'm developer!"/>
		<property name="link" value="http://ejemplo3.com"/>
	</bean>
	
</beans>
