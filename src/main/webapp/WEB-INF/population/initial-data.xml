<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
>

	<import resource="classpath:/WEB-INF/population/conversion-service.xml"/>
	
	<!-- Anonymous user -->

	<bean name="anonymousUserAccount" class="acme.framework.entities.UserAccount">
		<property name="username" value="anonymous" />
		<property name="password" value="anonymous" />
		<property name="enabled" value="false" />
		<property name="identity">
			<bean class="acme.datatypes.UserIdentity">
				<property name="name" value="John" />
				<property name="surname" value="Doe" />
				<property name="email" value="john.doe@acme.com" />
			</bean>
		</property>
		<property name="roles">
			<list>
				<ref bean="anonymousAnonymous" />
			</list>
		</property>
	</bean>

	<bean name="anonymousAnonymous" class="acme.framework.entities.Anonymous">
		<property name="userAccount" ref="anonymousUserAccount" />
	</bean>

	<!-- Administrators -->
	
	<bean name="administratorUserAccount" class="acme.framework.entities.UserAccount">
		<property name="username" value="administrator" />
		<property name="password" value="administrator" />
		<property name="enabled" value="true" />
		<property name="identity">
			<bean class="acme.datatypes.UserIdentity">
				<property name="name" value="Administrator" />
				<property name="surname" value="Acme.com" />
				<property name="email" value="administrator@acme.com" />
			</bean>
		</property>
		<property name="roles">
			<list>
				<ref bean="administratorAdministrator" />
				<ref bean="administratorAuthenticated" />
			</list>
		</property>
	</bean>

	<bean name="administratorAdministrator" class="acme.framework.entities.Administrator">
		<property name="userAccount" ref="administratorUserAccount" />
	</bean>
	
	<bean name="administratorAuthenticated" class="acme.framework.entities.Authenticated">
		<property name="userAccount" ref="administratorUserAccount" />
	</bean>
	
	<!-- ExecutionPeriod -->
	
	<bean id="finalizado" class="acme.datatypes.ExecutionPeriod">
        <property name="initialDate" value="04/12/21 10:15 AM"/>
        <property name="finalDate" value="04/21/21 3:30 PM"/>
    </bean>
	<bean id="noFinalizado" class="acme.datatypes.ExecutionPeriod">
        <property name="initialDate" value="05/12/21 10:15 AM"/>
        <property name="finalDate" value="07/12/21 3:30 PM"/>
    </bean>
    
    <!-- Task -->

    <bean id="task1" class="acme.entities.task.Task">
        <property name="title" value="anonymous 1"/>
        <property name="description" value="anonymous "/>
        <property name="workload" value="24.0"/>
        <property name="executionPeriod" ref="finalizado"/>
        <property name="url" value="https://github.com/serrojjim/dp2-june-16"/>
        <property name="isPrivate" value="false"/>
        <property name="workplan">
            <list>
                <ref bean="workplan1"/>
                <ref bean="workplan4"/>
                <ref bean="workplan5"/>
                <ref bean="workplan6"/>
                <ref bean="workplan7"/>
                <ref bean="workplan8"/>
                <ref bean="workplan9"/>
                <ref bean="workplan10"/>
            </list>
        </property>
        <property name="userAccount">
            <list>
                <ref bean="authenticatedUserAccount"/>
            </list>
        </property>
    </bean>
    
